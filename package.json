{
  "name": "project-temp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "DISABLE_ESLINT_PLUGIN=true NEXT_DISABLE_ESLINT=1 NEXT_IGNORE_TYPESCRIPT_ERRORS=true next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "prisma:seed": "ts-node prisma/seed.ts"
